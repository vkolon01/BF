<!DOCTYPE html>
<html lang="eng">
<head>
    <% include ./template/head.ejs %>
</head>
<body id="<% pageID %>">

<div id="wrapper">
    <header>
        <% include ./template/header.ejs%>
    </header>

    <div id="mainContent">
        <% include content/mainContent.ejs %>
        <ul id="browse">
            <img src=<% if(book.cover=="null"){%>"/images/null.jpg"<%}%>>
            <h2>Title: <%= book.title %></h2>
            <h3>Autor:  <%= book.autor%></h3>
            <p>Summary <br> <%= book.summary%></p>

            <textarea id="commentInput" type="text", maxlength="100"></textarea>
        </ul>
    </div>
</div>
<%include ./template/scripts.ejs%>
</body>
</html>
