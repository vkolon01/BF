<h1>
    Hello there citizen.
</h1>
<ul id="browse">
    <%if(featureBooks){
    for(var i = 0; i < featureBooks.length; i++){ %>
    <li>
        <%if(featureBooks[i]){%>
        <a href="<%='books/'+featureBooks[i]._id%>" title="goToBook">
            <img src=<% if(featureBooks[i].cover=="null"){%>"/images/null.jpg"<%}%>>
            <h2>Title: <%= featureBooks[i].title %></h2>
            <h3>Author:  <%if(bookAuthor[i]){%><%=bookAuthor[i]%><%}else{%>Author Not Specified<%}%></h3>
            <p>Summary <br> <%= featureBooks[i].summary%></p>
        </a></li>
    <% }}}else{%>
        <p>Click <a href="createBook">here</a> to add the very first book</p>
    <%}%>

</ul>